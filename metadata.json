    "about": "stackexchange-datasette",
    "about_url": "https://github.com/cldellow/stackexchange-to-sqlite",
    "databases": {
        "cooking": {
            "tables": {
                "badges": {
                    "facets": ["name"]
                },
                "users": {
                    "label_column": "display_name"
                }
            },
            "plugins": {
                "datasette-ui-extras": {
                    "editable": ["qtitles"]
                }
            }
        },
        "superuser": {
            "tables": {
                "badges": {
                    "facets": ["name"]
                },
                "users": {
                    "label_column": "display_name"
                }
            }
        }
    },
    "plugins": {
        "datasette-parquet": {
            "trove": {
                "directory": "./trove"
            }
        }
    }
}
